      "type": "y"
    }
  ],
  "marks": [
    {
      "type": "rect",
      "from": {
        "data": "table"
      },
      "properties": {
        "enter": {
          "x": {
            "scale": "xscale",
            "field": "state",
            "offset": 0,
            "mult": 0.0
          },
          "width": {
            "scale": "xscale",
            "band": true,
            "offset": -4,
            "mult": 0.0
          },
          "y": {
            "scale": "yscale",
            "field": "cancellation_num",
            "offset": 0,
            "mult": 0.0
          },
          "y2": {
            "scale": "yscale",
            "value": 0,
            "offset": 0,
            "mult": 0.0
          }
        },
        "update": {
          "fill": {
            "value": "steelblue",
            "offset": 0,
            "mult": 0.0
          }
        },
        "hover": {
          "fill": {
            "value": "Teal",
            "offset": 0,
            "mult": 0.0
          }
        }
      }
    },
    {
      "type": "text",
      "properties": {
        "enter": {
          "fill": {
            "value": "#333",
            "offset": 0,
            "mult": 0.0
          },
          "align": {
            "value": "center",
            "offset": 0,
            "mult": 0.0
          }
        },
        "update": {
          "x": {
            "scale": "xscale",
            "offset": 0,
            "mult": 0.0,
            "signal": "tooltip.state"
          },
          "y": {
            "scale": "yscale",
            "offset": -5,
