{
  "width": 800,
  "height": 550,
  "padding": "auto",
  "data": [
    {
      "name": "table",
      "values": []
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "ordinal",
      "range": "width",
      "nice": true,
      "domain": {
        "data": "table",
        "field": "state"
      }
    },
    {
      "name": "yscale",
      "range": "height",
      "nice": true,
      "domain": {
        "data": "table",
